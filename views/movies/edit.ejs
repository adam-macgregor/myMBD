<h2  class="page-header">Edit movie</h2>
<form action="/movies/<%= movie.id %>?_method=PUT" method="POST">
    <%- include('_form_fields') %> 
    <div class="btn-row form-row form-row-end">
        <%- include('../partials/deleteForm', { url: `/movies/${movie.id}`}) %> 
        <a class="btn btn-primary" href="/movies">Cancel</a>
        <button class="btn btn-primary" type="submit">Update movie</button>
    </div>
</form>