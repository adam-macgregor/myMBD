<% if(recentlyAddedMovies.length >= 1) { %> 
    <div>
        <h2 class="page-header">Recently Added Movies</h2>
        <div> 
        <% recentlyAddedMovies.forEach(recentMovie => { %>
            <div>
                <h4><%= recentMovie.title %></h4>
                <div class="btn-row">
                    <a class="btn btn-primary" href="/movies/<%= recentMovie.id %>">View</a>
                </div>
            </div>
        <% }) %>
        </div> 
        <hr>
    </div>
<% } %>