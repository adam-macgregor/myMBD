<h2  class="page-header"><%= list.name %> </h2>
<div class="btn-row">
    <a class="btn btn-primary" href="/lists/<%= list.id %>/edit">Edit</a>
</div>
<br>
<br>
<% movies.forEach(movie => { %>
    <div>
        <%= movie.title %>
        <div class="btn-row">
            <a class="btn btn-primary" href="/movies/<%= movie.id %>">View</a>
            <a class="btn btn-primary" href="/movies/<%= movie.id %>/edit">Edit</a>    
        </div>
    </div>
<% }) %>