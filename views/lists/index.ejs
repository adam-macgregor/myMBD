<h2>Lists</h2>
<form action="/lists" method="GET">
    <label>Name</label>
    <input type="text" name="name" value="<%= searchOptions.name %>">
    <button type="submit">Search</button>
</form>
<br>
<br>
<% lists.forEach( list => { %>
    <div>
        <%= list.name %>
        <a href="/lists/<%= list.id %>">View</a>
        <a href="/lists/<%= list.id %>/edit">Edit</a>
        <%- include('../partials/deleteForm', { url: `/lists/${list.id}`}) %> 
    </div>
<% }) %>